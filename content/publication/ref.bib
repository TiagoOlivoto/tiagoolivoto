journal = {Ciencia Rural},
keywords = {Experimental design,Experimental precision,Lycopersicon esculentum var. cerasiforme,Olericulture,Protected environment,cherry tomato,plot size,variability},
mendeley-tags = {cherry tomato,plot size,variability},
