pages = {230--242},
title = {{Variability, plot size and border effect in lettuce trials in protected environment}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0006-87052018005001101{\&}lng=en{\&}tlng=en},
