pages = {2331--2342},
title = {{Nonlinear Modeling for Analyzing Data from Multiple Harvest Crops}},
url = {https://dl.sciencesocieties.org/publications/aj/abstracts/0/0/agronj2018.05.0307 http://doi.wiley.com/10.2134/agronj2018.05.0307},
volume = {110},
