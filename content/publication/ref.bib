issn = {15921573},
journal = {Advances in Horticultural Science},
keywords = {Chicory,Growth rate,Radiation use efficiency,Shading screens,chicory,growth rate,radiation use efficiency,shading screens},
mendeley-groups = {Mestrado},
mendeley-tags = {chicory,growth rate,radiation use efficiency,shading screens},
number = {4},
pages = {257--265},
publisher = {University of Florence},
title = {{Solar radiation levels modify the growth traits and bromatological composition of cichorium intybus}},
volume = {31},
