journal = {Enciclop{\'{e}}dia Biosfera},
keywords = {Cucumber,path analysis},
mendeley-tags = {Cucumber,path analysis},
number = {21},
pages = {69--80},
title = {{An{\'{a}}lise de trilha para caracteres relacionados ao crescimento de mudas de pepineiro}},
volume = {11},
